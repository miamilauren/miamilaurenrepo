from bs4 import BeautifulSoup
import bs4 as bs
import urllib
import urllib.request
import requests
sauce = urllib.request.urlopen('https://firststop.sos.nd.gov/search/business').read()
soup = bs.BeautifulSoup(sauce, 'lxml')


##Third Try
import requests
from bs4 import BeautifulSoup
r = requests.get("https://firststop.sos.nd.gov/api/Records/businesssearc", params=dict(
        query="web scraping", 
        page=2
        ))
print (r.text)
